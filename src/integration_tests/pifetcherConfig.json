{
  "browser": {
    "browser_options": [
      "--window-size=1920,1080",
      "--disable-extensions",
      "--proxy-server='direct://'",
      "--proxy-bypass-list=*",
      "--start-maximized",
      "--ignore-certificate-errors",
      "--headless",
      "--no-sandbox",
      "--disable-dev-shm-usage"
    ],
    "win-driver_path": "chromedriver-win-80.exe",
    "win-binary_location": "",
    "mac-driver_path": "chromedriver-mac-80",
    "mac-binary_location": "",
    "linux-driver_path": "chromedriver-linux-80",
    "linux-binary_location": ""
  },
  "driverType": "Pyppeteer",
  "queue": {
    "numWorksPerTime": 1,
    "queueType": "LocalWorkQueue",
    "queueName": "",
    "pollingIntervalOnActive": 0.5,
    "pollingIntervalOnIdle": 3
  },
  "logger": {
    "output": "console"
  },
  "fetcher": {
    "mappingConfigs": {
      "amazon": "amazon.json"
    }
  }
}